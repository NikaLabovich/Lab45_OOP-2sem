<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:OOP4_5">
    <Style x:Key="MenuStyle" TargetType="Menu">
        <Setter Property="Background" Value="#72C58F" />
        <Setter Property="Foreground" Value="Black" />
    </Style>
    <Style x:Key="ToolBarStyle" TargetType="ToolBar">
        <Setter Property="Background" Value="#72C58F" />
        <Setter Property="Foreground" Value="Black" />
    </Style>
    <Style x:Key="TextBlockStyle" TargetType="RichTextBox">
        <Setter Property="Background" Value="White" />
        <Setter Property="Foreground" Value="Black" />
    </Style>
    <Style x:Key="StatusBarStyle" TargetType="StatusBar">
        <Setter Property="Background" Value="#72C58F" />
        <Setter Property="Foreground" Value="Black" />
    </Style>
    <Style x:Key="LabelStyle" TargetType="Label">
     
        <Setter Property="Foreground" Value="Black" />
    </Style>
</ResourceDictionary>